[application]

config/name="Goktasi"
run/main_scene="res://Main/Main.tscn"
config/icon="res://Images/goktasi_icon.png"
config/quit_on_go_back=false

[autoload]

GameState="*res://Main/GameState.gd"
Random="*res://Random/Random.gd"
ItemDB="*res://ItemDB/ItemDB.tscn"
GlobalParticles="*res://Particles/Particles.tscn"
Player="*res://Player/Player.gd"
DataBase="*res://DataBase/DataBase.gd"
Console="*res://Console/Console.gd"
Map="*res://Map/Map.gd"
PlayerInventory="*res://Player/PlayerInventory.gd"
Store="*res://Store/Store.gd"
ConsoleGUI="*res://Console/ConsoleGUI.tscn"
Notification="*res://Notification/Notification.gd"
InfoPanel="*res://GUI/InfoPanel/InfoPanel.tscn"

[display]

window/size/width=600
window/size/height=1024
window/size/fullscreen=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

